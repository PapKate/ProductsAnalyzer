<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.18.0" />
    <PackageReference Include="IronXL.Excel" Version="2023.1.14" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Atom">
      <HintPath>bin\Debug\net7.0\Atom.dll</HintPath>
    </Reference>
    <Reference Include="Atom.Data">
      <HintPath>bin\Debug\net7.0\Atom.Data.dll</HintPath>
    </Reference>
    <Reference Include="Atom.Data.Excel">
      <HintPath>bin\Debug\net7.0\Atom.Data.Excel.dll</HintPath>
    </Reference>
    <Reference Include="Atom.Shortcodes">
      <HintPath>bin\Debug\net7.0\Atom.Shortcodes.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
